<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gongbo.fss.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".adapter.AdapterActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".adapter.listview.BaseSimpleAdapterTestActivity" />
        <activity android:name=".adapter.listview.BaseBindSimpleAdapterTestActivity" />
        <activity android:name=".adapter.listview.BaseBindingSimpleAdapterTestActivity" />
        <activity android:name=".adapter.listview.BaseBindingSimpleSingleAdapterTestActivity" />
        <activity android:name=".adapter.listview.CommonAdapterTestActivity" />
        <activity android:name=".adapter.recyclerview.BaseRecyclerViewAdapterTestActivity" />
        <activity android:name=".adapter.recyclerview.BaseBindRecyclerViewAdapterTestActivity" />
        <activity android:name=".adapter.recyclerview.BaseBindingRecyclerViewAdapterTestActivity" />
        <activity android:name=".adapter.recyclerview.CommonRecyclerAdapterViewTestActivity" />
        <activity android:name=".runpriority.RunPriorityTestActivity" />
        <activity android:name=".bind.BindDetailActivity">
            <intent-filter>
                <action android:name="com.gongbo.fss.bind.detail" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".bind.BindTestActivity" />
        <activity android:name=".route.RouteMainActivity" />
        <activity android:name=".route.RouteDetailActivity" >
            <intent-filter>
                <action android:name="com.gongbo.fss.route.detail" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service
            android:name=".route.service.RouteTestService"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>